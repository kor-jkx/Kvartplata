              column="WorkTime" />
    <property name="WorkTimeCash"
          column="WorkTimeCash" />
    <property name="WorkPlaceCash"
          column="WorkPlaceCash" />
    <property name="Address"
              column="Address"  />
    <property name="SocOrgId"
          column="SocOrg_Id"  />
    <property name="UName" column="Uname"  />
    <property name="DEdit" column="Dedit"  />

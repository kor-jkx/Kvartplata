<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Kvartplata"
                   namespace="Kvartplata.Classes">

  <class name="Receipt" table="DBA.dcReceipt">
    <id name="ReceiptId" >
      <column name="Receipt_id" not-null="true" />
      <generator class="assigned"/>
    </id>
    <property name="ReceiptName"
              column="Receipt_Name"
              not-null="true"/>
    <property name="UName"
      column="UName"
      not-null="true"/>
    <property name="DEdit"
  column="DEdit"
  not-null="true"/>
  </class>

</hibernate-mapping>
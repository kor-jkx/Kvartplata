<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Kvartplata"
                   namespace="Kvartplata.Classes">

  <class name="ClassTable" table="DBA.srvClassTable">
    <id name="TableId" unsaved-value="0">
      <column name="Table_id" not-null="true" />
      <generator class="identity"/>
    </id>
    <property name="ClassName"
              column="Class_Name"/>
    <property name="ClassValueField"
             column="Field_Value"/>
    <property name="ClassDisplayField"
             column="Field_Display"/>
  </class>

</hibernate-mapping>
